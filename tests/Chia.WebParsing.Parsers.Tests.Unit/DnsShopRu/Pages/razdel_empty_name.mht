r .ui-state-default {
    background: #E6E6E6;
    border: 1px solid #BBBBBB;
    color: #555555;
    font-weight: normal;
}*/

.ui-slider .ui-slider-handle {
    cursor: default;
    height: 10px;
    position: absolute;
    width: 10px;
    z-index: 0;
}

.ui-slider-horizontal .ui-slider-handle {
    margin-left: -5px;
    top: -5px;
}

.ui-slider .ui-slider-range {
    background-position: 0 0;
    border: 0 none;
    display: block;
    font-size: 0.7em;
    position: absolute;
    z-index: 0;
}

.ui-widget-content {
    background: #FFFFFF;
    border: 1px solid #AAAAAA;
    color: #222222;
}

.ui-widget-header {
    background: #FAA627;
    border: 1px solid #AAAAAA;
    color: #222222;
    font-weight: bold;
}

