sHistory)){

                       var identifier =3D "&identifier=3D" + (new Date()).g=
etTime();
                      =20
                      =20
                      var historyObject =3D new categoryDisplayJS.HistoryTr=
acker('CategoryDisplay_Widget', controller.url + identifier);
                       dojo.back.addToHistory(historyObject);      =20
                       categoryDisplayJS.contextChanged =3D false;
                       categoryDisplayJS.isHistory =3D false; =20
