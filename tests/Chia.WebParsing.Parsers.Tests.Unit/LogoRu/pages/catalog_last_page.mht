(function($){

	// Number of seconds in every time division
	var days	=3D 24*60*60,
		hours	=3D 60*60,
		minutes	=3D 60;

	// Creating the plugin
	$.fn.countdown =3D function(prop){

		var options =3D $.extend({
			callback	: function(){},
			timestamp	: 0,
			nowstamp	: 0
		},prop);

		var left, d, h, m, s, positions;

		// Initialize the plugin
		init(this, options);

		positions =3D this.find('.position');

		(function tick(){

			// Time left
			left =3D Math.floor((options.timestamp - options.nowstamp) / 1000);
			options.nowstamp =3D Number(options.nowstamp) + 1000;
			if(left < 0){
				left =3D 0;
			}

			// Number of days left
			d =3D Math.floor(left / days);
			updateDuo(0, 1, d);
			left -=3D d*days;

			// Number of hours left
			h =3D Math.floor(left / hours);
			updateDuo(2, 3, h);
			left -=3D h*hours;

			// Number of minutes left
			m =3D Math.floor(left / minutes);
			updateDuo(4, 5, m);
			left -=3D m*minutes;

			// Number of seconds left
			s =3D left;
			updateDuo(6, 7, s);

			// Calling an optional user supplied callback
			options.callback(d, h, m, s);

			// Scheduling another call of this function in 1s
			setTimeout(tick, 1000);
		})();

		// This function updates two digit positions at once
		function updateDuo(minor,major,value){
			switchDigit(positions.eq(minor),Math.floor(value/10)%10);
			switchDigit(positions.eq(major),value%10);
		}

		return this;
	};


	function init(elem, options){
		elem.addClass('countdownHolder');

		// Creating the markup inside the container
		$.each(['Days','Hours','Minutes','Seconds'],function(i){
			$('<span class=3D"count'+this+'">').html(
				'<span class=3D"position"><span class=3D"digit static">0</span></span><=
span class=3D"position"><span class=3D"digit static">0</span></span>'
			).appendTo(elem);

			if(this!=3D"Seconds" && this!=3D "Days"){
				elem.append('<span class=3D"countDiv countDiv'+i+'">:</span>');
			}
		});

	}

	// Creates an animated transition between the two numbers
	function switchDigit(position,number){

		var digit =3D position.find('.digit')

		if(digit.is(':animated')){
			return false;
		}

		if(position.data('digit') =3D=3D number){
			// We are already showing this number
			return false;
		}

		position.data('digit', number);

		var replacement =3D $('<span>',{
			'class':'digit',
			css:{
				top:'-2.1em',
				opacity:0
			},
			html:number
		});

		// The .static class is added when the animation
		// completes. This makes it run smoother.

		digit
			.before(replacement)
			.removeClass('static')
			.remove();


		replacement
			.delay(100)
			.animate({top:0,opacity:1},'fast',function(){
				replacement.addClass('static');
			});
	}
})(jQuery);
