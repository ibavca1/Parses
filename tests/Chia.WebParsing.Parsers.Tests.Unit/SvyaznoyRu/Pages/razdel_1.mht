function number_format( number, decimals, dec_point, thousands_sep )
{
    var i, j, kw, kd, km;
    if( isNaN(decimals =3D Math.abs(decimals)) ){decimals =3D 2;}
    if( dec_point =3D=3D undefined ){dec_point =3D ",";}
    if( thousands_sep =3D=3D undefined ){thousands_sep =3D ".";}
    i =3D parseInt(number =3D (+number || 0).toFixed(decimals)) + "";
    if( (j =3D i.length) > 3 ){j =3D j % 3;} else{j =3D 0;}
    km =3D (j ? i.substr(0, j) + thousands_sep : "");
    kw =3D i.substr(j).replace(/(\d{3})(?=3D\d)/g, "$1" + thousands_sep);
    kd =3D (decimals ? dec_point + Math.abs(number - i).toFixed(decimals).r=
eplace(/-/, 0).slice(2) : "");
    return km + kw + kd;
}

