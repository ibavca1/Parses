    // Get the container for the suggestions=20
    // (the one to put the suggestions to)
    m_arObject['container'] =3D $("#"+mc_szContainerID);

    // Get the suggestions block (the one to display or hide)
    m_arObject['suggestion'] =3D $("#"+mc_szParentID);

    // Get the search line (input area for the user's search request)
    m_arObject['input'] =3D $("#"+mc_szQueryID);

    // Check if this form supports suggestions
    if (m_arObject['container'].length < 1 || m_arObject['input'].length < =
