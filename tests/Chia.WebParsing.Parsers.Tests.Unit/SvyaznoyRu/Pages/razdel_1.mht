    // Right after the user typed the next character
    m_arObject['input'].keyup(function(event) {
        if (event.keyCode !=3D 38 && event.keyCode !=3D 40 && event.keyCode=
 !=3D 13) {
            m_arObject['suggestion'].hide();
        }
       =20
        if($(this).val().length < 2)  {
            m_arObject['suggestion'].hide();
            m_iLastCount =3D $(this).val().length;
            clearTimeout(suggestion_timer);
            return;
        }
       =20
        suggestion_event_key =3D event.keyCode;
        suggestion_event =3D event;
        suggestion_obj =3D $(this);

        clearTimeout(suggestion_timer);
        //suggestion_timer =3D setTimeout("loadSuggestion()", 2000);
        suggestion_timer =3D setTimeout("loadSuggestion()", 500);
        return false;
    });

    // User is no more interested in searching anything
    HideOnBlur(true);
});

function loadSuggestion()
{
    var event =3D suggestion_event;
   =20
    if (event =3D=3D null)
        return;

    var szText =3D $(suggestion_obj).val();

    if ( event.keyCode =3D=3D 13 ) {
        var li =3D $("#search_suggestion #search_suggestion_container ul").=
children("li.active");
        if (li.attr("rel")) {
            window.location =3D li.attr("rel");
        }
        return false;
    }

    if (event.keyCode =3D=3D 38 || event.keyCode =3D=3D 40 || event.keyCode=
 =3D=3D 13) {
        var li =3D $("#search_suggestion #search_suggestion_container ul").=
children("li");
        li.removeClass("active");
        switch (event.keyCode) {
            case 38:
                currentElement--;
                if (currentElement < 0) {
                    currentElement =3D li.length - 1;
                }
            break;
            case 40:
                if (!first) {
                    currentElement++;
