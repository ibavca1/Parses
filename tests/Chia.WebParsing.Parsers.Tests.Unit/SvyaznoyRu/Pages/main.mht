Content-Transfer-Encoding: quoted-printable
Content-Type: text/css

div.banner_rotator
{
    width:620px;
    height:286px;
    border: 0px solid #c0c0c0;
    border-radius: 0 0 6px 6px;
    -moz-border-radius: 0 0 6px 6px;
    -webkit-border-radius: 0 0 6px 6px;
    position:relative;
    background-color:#f0f0f0;
    overflow: hidden;
    background-image:url(bg-br.png);
    background-repeat: repeat-x;
    background-position: bottom left;
}
div.banner_rotator div.banners  div.banner
{
    display:none;
}
div.banner_rotator div.banners  div.banner img
{
    border:0;
}
div.banner_rotator div.buttons
{
    position:absolute;
    top:258px;
    left:0px;
}
div.banner_rotator div.buttons div
{
    float:left;
    border-top:1px solid #FFFFFF;
    border-right:1px solid #C0C0C0;
    border-bottom:0;
    border-left:1px solid #FFFFFF;
    width:38px;
    height:26px;
}
div.banner_rotator div.buttons div.control
{
    border-left:0;
    cursor: pointer;
    //cursor: hand;
}
div.banner_rotator div.buttons div.play
{
    background-image:url(b-play.png);
    background-repeat: no-repeat;
    background-position: center center;
}
div.banner_rotator div.buttons div.pause
{
    background-image:url(b-pause.png);
    background-repeat: no-repeat;
    background-position: center center;
}
div.banner_rotator div.buttons div.button
{
    padding-left:14px;
    padding-top:4px;
    width:24px;
    height:22px;
    font-weight:bold;
    font-family:Verdana;
    font-size:10pt;
    text-shadow:#A0A0A0 1px 1px 1px;
    color:#505050;
    cursor: pointer;
    //cursor: hand;
}
div.banner_rotator div.buttons div.active
{
    font-size:12pt;
}
div.fade
{
    position:absolute;
    top:0px;
    left:0px;
    background-color:#FFFFFF;
    width:620px;
    height:260px;
    display:none;
}

