var banner_rotator =3D new Object();
banner_rotator.xmlurl =3D "xml/banner_settings.xml";
banner_rotator.banners =3D new Array();
banner_rotator.timeout =3D null;
banner_rotator.current =3D null;
banner_rotator.div =3D "";
banner_rotator.getBanners =3D function()
{
	$.ajax({
		url: this.xmlurl,
		dataType: 'xml',
		error: function(msg){
			alert(msg);
		},
		success: function(data){
			$(data).find("items item").each(function(){
				var reg=3D/(jpe|jpeg|jpg|gif|png)$/i
				banner =3D new Object();
				banner.file =3D $(this).find("file").text();
				if (!reg.test(banner.file)) return;
				banner.link =3D $(this).find("link").text();
				banner.time =3D Number($(this).find("itemSettings").attr("slideshowInte=
rval"));
				banner.target =3D $(this).find("itemSettings").attr("targetLink");
				banner_rotator.banners.push(banner);
			});
			banner_rotator.render();
			banner_rotator.nextshow();
		}
	});
};
banner_rotator.dropTimeout =3D function()
{
	$("#"+this.div+" div.buttons div.control").removeClass("play");
	$("#"+this.div+" div.buttons div.control").removeClass("pause");
	$("#"+this.div+" div.buttons div.control").addClass("play");
	if (this.timeout !=3D null)
	{
		clearTimeout(this.timeout);
		this.timeout =3D null;
	}
};
banner_rotator.render =3D function()
{
	var i;
	var output;
	for(i=3D0;i<this.banners.length;i++)
	{
		output =3D "";
		output +=3D "<div class=3D\"banner\"><a href=3D\""+this.banners[i].link+"=
\""+((this.banners[i].target!=3D"")?" target=3D\""+this.banners[i].target+"=
\"":"")+"><img src=3D\""+this.banners[i].file+"\" /></a></div>";
		$("#"+this.div+" div.banners").append(output);

		output =3D "";
		output +=3D "<div class=3D\"button\">"+(i+1)+"</div>";
		$("#"+this.div+" div.buttons").append(output);
	}
	$("#"+this.div+" div.buttons div.button").click(function(event){
		banner_rotator.clickindex($(this));
		event.stopPropagation();
	});
	$("#"+this.div+" div.buttons div.control").click(function(event){
		banner_rotator.clickcontrol();
		event.stopPropagation();
	});
};
banner_rotator.setindex =3D function(i)
{
	this.dropTimeout();
	if (i < 0) return;
	if (i =3D=3D this.current) return;
	$("#"+this.div+" div.buttons div.button").removeClass("active");
	$("#"+this.div+" div.fade").attr("id",i);
	$("#"+this.div+" div.fade").fadeIn(100,function(){
		var i =3D Number($(this).attr("id"));
		$("#"+banner_rotator.div+" div.banners div.banner").hide();
		$("#"+banner_rotator.div+" div.banners div.banner").eq(i).show();
		$("#"+banner_rotator.div+" div.buttons div.button").eq(i).addClass("activ=
e");
		$(this).attr("id","");
		$(this).fadeOut(100);
	});
	this.current =3D i;
};

banner_rotator.clickindex =3D function(obj)
{
	var i =3D $("#"+this.div+" div.buttons div.button").index(obj);
	this.setindex(i);
	return;
};
banner_rotator.clickcontrol =3D function(obj)
{
	if (this.timeout !=3D null)
	{
		this.pauseshow();
	}
	else
	{
		this.playshow();
	}
};
banner_rotator.nextshow =3D function()
{
	var next_i =3D 0;
	if (this.current =3D=3D null) next_i =3D 0;
	else next_i =3D this.current+1;
	if (next_i > this.banners.length-1) next_i =3D 0;
	this.setindex(next_i);
	this.timeout =3D setTimeout("banner_rotator.nextshow();",this.banners[next=
_i].time*1000);
	$("#"+this.div+" div.buttons div.control").removeClass("play");
	$("#"+this.div+" div.buttons div.control").removeClass("pause");
	$("#"+this.div+" div.buttons div.control").addClass("pause");
};
banner_rotator.playshow =3D function()
{
	this.timeout =3D setTimeout("banner_rotator.nextshow();",this.banners[this=
.current].time*1000);
	$("#"+this.div+" div.buttons div.control").removeClass("play");
	$("#"+this.div+" div.buttons div.control").removeClass("pause");
	$("#"+this.div+" div.buttons div.control").addClass("pause");
};
banner_rotator.pauseshow =3D function()
{
	this.dropTimeout();
	$("#"+this.div+" div.buttons div.control").removeClass("play");
	$("#"+this.div+" div.buttons div.control").removeClass("pause");
	$("#"+this.div+" div.buttons div.control").addClass("play");
};
banner_rotator.init =3D function(div)
{
	this.div =3D div;
	var output =3D "";
	output +=3D "<div class=3D\"fade\"></div>";
	output +=3D "<div class=3D\"banners\">";
	output +=3D "</div>";
	output +=3D "<div class=3D\"buttons\">";
	output +=3D "<div class=3D\"control\"></div>";
	output +=3D "</div>";
	$("#"+div).html(output);
	this.getBanners();
};

