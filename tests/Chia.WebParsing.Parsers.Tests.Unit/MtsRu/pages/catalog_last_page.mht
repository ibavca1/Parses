				if (data.status =3D=3D 'success') {
					$('#subscribe_block .errors').addClass("hidden");
					$('#subscriber_name').text(data.subscriber.name);
					$('#subscribe_form').addClass('hidden');
					$('#subscribe_success').removeClass('hidden');
				} else {
					if (data.status =3D=3D 'error') {
						$('#subscribe_block .errors').addClass("hidden");
						$('#subscribe_block input').removeClass('subscribe_field_error');
						for (i in data.errors) {
							errors =3D "";
							for (e in data.errors[i]) {
								errors +=3D '<li>' + data.errors[i][e] + '</li>'
							}
							$('#subscribe_' + i + '_errors').html(errors);
							$('#subscribe_' + i + '_errors').removeClass("hidden");
							$('#subscribe_' + i).addClass('subscribe_field_error');

						}
					}
				}
			}
		});

		return false;

	});
}
