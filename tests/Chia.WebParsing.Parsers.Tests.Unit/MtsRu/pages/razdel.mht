    if(!level) level =3D 0;
    var level_padding =3D "";
    for(var j=3D0; j<level+1; j++) level_padding +=3D "    ";
    if(typeof(arr) =3D=3D 'object') {
        for(var item in arr) {
            var value =3D arr[item];
            if(typeof(value) =3D=3D 'object') {
                print_red_text +=3D level_padding + "'" + item + "' :\n";
                print_red_text +=3D print_r(value,level+1);
		}
            else
                print_red_text +=3D level_padding + "'" + item + "' =3D> \"=
" + value + "\"\n";
        }
    }

    else  print_red_text =3D "=3D=3D=3D>"+arr+"<=3D=3D=3D("+typeof(arr)+")";
    return print_red_text;
}
