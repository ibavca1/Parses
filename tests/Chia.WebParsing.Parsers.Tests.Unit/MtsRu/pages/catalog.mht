		if(!$(this).find('input:checkbox').attr('checked')){
			allchkd =3D false;
		}
	});
	if(allchkd){
		$('#all_checked').addClass('allchecked');
	}

	$('#all_checked').click(function(){
		var ul =3D $(this).parent().parent().find('ul');
		ul =3D $(ul);
		var remove, add, check;
		if($(this).hasClass('allchecked')){
			remove =3D 'filtr_cheked filtr_hover';
			add =3D 'filtr_nochek';
			check =3D false;
			$(this).removeClass('allchecked');
		} else {
			remove =3D 'filtr_nochek';
			add =3D 'filtr_cheked filtr_hover';
			check =3D 'checked';
			$(this).addClass('allchecked');
		}
			ul.find('li').each(function(){
				$(this).find('input:checkbox').attr('checked', check);
				$(this).find('.filtr_chek').removeClass(remove).addClass(add);
			});
			$("form.#filter_form input:first").trigger('change');
	});


