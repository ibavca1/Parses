BX.ajax.prepareForm =3D function(obForm, data)
{
	data =3D (!!data ? data : {});
	var i, ii, el,
		_data =3D [],
		n =3D obForm.elements.length,
		files =3D 0, length =3D 0;
	if(!!obForm)
	{
		for (i =3D 0; i < n; i++)
		{
			el =3D obForm.elements[i];
			if (el.disabled)
				continue;
			switch(el.type.toLowerCase())
			{
				case 'text':
				case 'textarea':
				case 'password':
				case 'hidden':
				case 'select-one':
					_data.push({name: el.name, value: el.value});
					length +=3D (el.name.length + el.value.length);
					break;
				case 'file':
					if (!!el.files)
					{
						for (ii =3D 0; ii < el.files.length; ii++)
						{
							files++;
							_data.push({name: el.name, value: el.files[ii], file : true});
							length +=3D el.files[ii].size;
						}
					}
					break;
				case 'radio':
				case 'checkbox':
					if(el.checked)
					{
						_data.push({name: el.name, value: el.value});
						length +=3D (el.name.length + el.value.length);
					}
					break;
				case 'select-multiple':
					for (var j =3D 0; j < el.options.length; j++)
					{
						if (el.options[j].selected)
						{
							_data.push({name : el.name, value : el.options[j].value});
							length +=3D (el.name.length + el.options[j].length);
						}
					}
					break;
				default:
					break;
			}
		}

		i =3D 0; length =3D 0;
		var current =3D data;

		while(i < _data.length)
		{
			var p =3D _data[i].name.indexOf('[');
			if (p =3D=3D -1) {
				current[_data[i].name] =3D _data[i].value;
				current =3D data;
				i++;
			}
			else
			{
				var name =3D _data[i].name.substring(0, p);
				var rest =3D _data[i].name.substring(p+1);
				if(!current[name])
					current[name] =3D [];

				var pp =3D rest.indexOf(']');
				if(pp =3D=3D -1)
				{
					current =3D data;
					i++;
				}
				else if(pp =3D=3D 0)
				{
					//No index specified - so take the next integer
					current =3D current[name];
					_data[i].name =3D '' + current.length;
				}
				else
				{
					//Now index name becomes and name and we go deeper into the array
					current =3D current[name];
					_data[i].name =3D rest.substring(0, pp) + rest.substring(pp+1);
				}
			}
		}
	}
	return {data : data, filesCount : files, roughSize : length};
};
