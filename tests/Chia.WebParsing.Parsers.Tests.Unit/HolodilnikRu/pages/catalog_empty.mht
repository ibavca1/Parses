/**
 * special offers tooltip functionality
 */
//settings
var tooltipMaxWidth =3D 525;
var processingSelectorClass =3D 'so'

//global variables
var tiptipTimer;
var clickObjects =3D new Array();

/**
 * Main plugin function. Creates divs and handlers. Runs once on document.r=
eady or after each ajax products list loading
 */
function applyTooltip() {
    var scope =3D 'body'
    if (arguments.length > 0) {
        scope =3D $(arguments[0])
    }

    var tmp =3D $('.so_tooltip', scope);
    if (tmp.length > 0) {
        //tooltips are ready
        return;
    }

    //rendering hidden divs for selector
    var browser =3D navigator.userAgent;
    var tmp =3D $('.' + processingSelectorClass, scope);
    for (var i =3D 0; i < tmp.length; i++) {
        if (!$(tmp[i]).attr('title')) {
            //tooltip ready
            continue;
        }

        var tooltipPosition =3D $(tmp[i]).attr('tooltip'); //rigth|left & t=
op|bottom ('right bottom', for example)
        var locations =3D tooltipPosition.split(' '); //rigth|left & top|bo=
ttom

        //background-image
        switch (tooltipPosition) {
            case 'right top':
                var _backgroundUrl =3D '/i/blok_calcul_bottom.png';
                break;
            case 'right bottom':
                var _backgroundUrl =3D '/i/blok_calcul_bottom_right.png';
                break;
            case 'left top':
                var _backgroundUrl =3D '/i/blok_calcul_top_left.png';
                break;
            case 'left bottom':
                var _backgroundUrl =3D '/i/blok_calcul_top.png';
                break;
        }

        //background-position for V and its X-location on container
        if (locations[0] =3D=3D 'left') {
            var bgXPos =3D '-270px';
            var textAlign =3D 'right';
            var marginLeft =3D ''
        } else {
            var bgXPos =3D '0px';
            var textAlign =3D 'left';
            var marginLeft =3D 'margin-left: 5px;'
        }

        //Opera and Firefox/Waterfor margins specify
        if ((parseInt(browser.indexOf('Opera')) + 1) || (parseInt(browser.i=
ndexOf('opera')) + 1)
            ||(parseInt(browser.indexOf('Firefox')) + 1) || (parseInt(brows=
er.indexOf('firefox')) + 1)
            ||(parseInt(browser.indexOf('Waterfox')) + 1) || (parseInt(brow=
ser.indexOf('waterfox')) + 1) )
        {
            if ((parseInt(browser.indexOf('Opera')) + 1) || (parseInt(brows=
er.indexOf('opera')) + 1)) {
                var marginBottom =3D ' margin-bottom: -11px;'; //text div (=
only for top Y-position)
            }
            var vMarginBottom =3D '3px'; //v div (only for bottom position)
        } else {
            var marginBottom =3D '';
            if (locations[0] =3D=3D 'left') {
                var vMarginBottom =3D '0px';
            } else {
                var vMarginBottom =3D '1px';
            }
        }

        //Container
        var newElem =3D document.createElement("div");
        newElem.className =3D "so_tooltip";
        newElem.style.position =3D 'absolute';
        newElem.style.textAlign =3D textAlign;
        newElem.style.maxWidth =3D tooltipMaxWidth + 'px';
        newElem.style.display =3D 'none';
        newElem.style.zIndex =3D 10;
        //newElem.style.display =3D 'block';

        //Common text
        var tooltipWinText =3D '<div style=3D"background: #EBF3F9; border: =
1px solid #7C99B9; z-index: 2; '
            + 'box-shadow: 0 0 3px #555555; border-radius: 6px; padding: 12=
px 20px 20px; text-align: left; '
            + 'margin-top: -4px; min-width: 515px;' + marginBottom + '" cla=
ss=3D"so_content">'
            + '<div style=3D"width: 100%; height: 16px">'
            + '<div style=3D"float: right; margin-right: -8px;"><img src=3D=
"/i/close_bg.jpg" style=3D"cursor: pointer; margin: 0px;" '
            + 'onclick=3D"tooltipMouseOutHandler()"></div></div>'
            + '<div style=3D"clear: both"></div>'
            + $(tmp[i]).attr('title')
            + '</div>';

        //Text + V
        if (locations[1] =3D=3D 'top') {
            newElem.innerHTML =3D tooltipWinText
                + '<div class=3D"credit_top" style=3D"background-image: url=
(' + _backgroundUrl + '); '
                + 'margin-top: -1px; position: relative; width: 120px; disp=
lay: inline-table; '
                + 'height: 38px; background-position: ' + bgXPos + ' bottom=
; ' + marginLeft + '"></div>'
        } else {
            newElem.innerHTML =3D '<div class=3D"credit_top" style=3D"backg=
round-image: url(' + _backgroundUrl + '); '
                + 'margin-bottom: ' + vMarginBottom + '; position: relative=
; width: 120px; display: inline-table; '
                + 'height: 38px; background-position: ' + bgXPos + ' top; '=
 + marginLeft + '"></div>' + tooltipWinText
        }


        //document.body.appendChild(newElem);
        $(newElem).insertAfter($(tmp[i]));
        $(tmp[i]).attr('title', '');
    }

    //interactive handlers
    $('*').click(function (e) {
        if ($(this).hasClass(processingSelectorClass)) {
            clickObjects.push(processingSelectorClass);
        } else {
            clickObjects.push($(this).attr('class'));
        }
        clearTimeout(tiptipTimer);
        tiptipTimer =3D setTimeout(anythingClickHandler, 50);
    });

    $('.' + processingSelectorClass, scope).click(function () {
        //check for already opened tooltip
        if ($(this).attr('tooltip_opened') =3D=3D 1) {
            //clearTimeout(tiptipTimer);
            //tiptipTimer =3D setTimeout(tooltipMouseOutHandler, 3000);
            return;
        }
        tooltipMouseOutHandler(); //hide visible tooltips
        $(this).attr('tooltip_opened', 1);

        clearTimeout(tiptipTimer);
        //dynamic width
