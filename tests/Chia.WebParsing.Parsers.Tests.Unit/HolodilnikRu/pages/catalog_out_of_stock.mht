function resizeMe (id, maxHeight) {
  var div =3D $('#' + id);
  if (div) {
    div.css('visibility', 'hidden').css('display', 'block');

    var elements =3D new Array();
    var summ =3D 0;
    var i =3D 0;
    var j =3D 0;
    elements[0] =3D new Array();

    $('a', div).each(function () {
      var a =3D $(this);
      if (summ + a.outerHeight() >=3D maxHeight) {
        i++; summ =3D 0;
        elements[i] =3D new Array();
      }
      elements[i].push(a);
      summ +=3D a.outerHeight();
    });


    for (i =3D 0; i < elements.length; i++) {
      var last =3D elements[i][elements[i].length - 1];
      var j =3D i + 1;

      if (j < elements.length) {
        var next  =3D elements[j][0];
        var next2 =3D elements[j][1];
        if (!$(last).hasClass('inner') && $(next).hasClass('inner')) {
          // =E4=EE=E1=E0=E2=E8=F2=FC =EF=E5=F0=E2=FB=EC
          elements[j].unshift(last);
          // =F3=E1=F0=E0=F2=FC =EF=EE=F1=EB=E5=E4=ED=E8=E9
          elements[i].pop();
        } else if ($(last).hasClass('inner') && $(next).hasClass('inner')) {
          if (!$(next2).hasClass('inner')) {
            // =F3=E1=F0=E0=F2=FC =EF=E5=F0=E2=FB=E9 =E8=E7 =F1=EB=E5=E4=F3=
=FE=F9=E5=E3=EE =F1=F2=EE=EB=E1=F6=E0
            elements[j].shift();
            // =EF=EE=F1=F2=E0=E2=E8=F2=FC =EF=EE=F1=EB=E5=E4=ED=E8=EC
            elements[i].push(next);
          }
        }
      }
    }

    // =F1=EE=E7=E4=E0=B8=EC =F2=E0=E1=EB=E8=F6=F3
    div.html(' ');
    div.append('<table><tr></tr></table>');
    var tr =3D div.find('tr');

    // =E7=E0=EF=EE=EB=ED=FF=E5=EC =F1=F2=EE=EB=E1=F6=FB
    for (i=3D0; i < elements.length; i++) {
