	this.ACTIVE_CLASS =3D arParams.ACTIVE_CLASS || '';
	this.LEVEL =3D arParams.LEVEL || 0;

	this.CLOSE_ON_CLICK =3D typeof arParams.CLOSE_ON_CLICK !=3D 'undefined' ? =
!!arParams.CLOSE_ON_CLICK : true;
	this.ADJUST_ON_CLICK =3D typeof arParams.ADJUST_ON_CLICK !=3D 'undefined' =
? !!arParams.ADJUST_ON_CLICK : true;

	this.TYPE =3D this.PARAMS.TYPE =3D=3D 'hover' ? 'hover' : 'click';

	this._openTimeout =3D null;

	if (this.PARAMS.TYPE =3D=3D 'hover' && arParams.TIMEOUT !=3D=3D 0)
		this.TIMEOUT =3D arParams.TIMEOUT || 1000;
	else
		this.TIMEOUT =3D 0;

	if (!!this.PARAMS.MENU_URL)
	{
		this.bMenuLoaded =3D false;
		this.bMenuLoading =3D false;

		this.MENU =3D [{
			TEXT: BX.message('JS_CORE_LOADING'),
			CLOSE_ON_CLICK: false
		}];

		if (this.PARAMS.MENU_PRELOAD)
		{
			BX.defer(this.Load, this)();
		}
	}

	BX.ready(BX.defer(this.Init, this));
};

BX.COpener.prototype.Init =3D function()
{
	this.DIV =3D BX(this.DIV);

	switch (this.TYPE)
	{
		case 'hover':
			BX.bind(this.DIV, 'mouseover', BX.proxy(this.Open, this));
			BX.bind(this.DIV, 'click', BX.proxy(this.Toggle, this));
		break;

		case 'click':
			BX.bind(this.DIV, 'click', BX.proxy(this.Toggle, this));
		break;
	}

	//BX.bind(window, 'scroll', BX.delegate(this.__close_immediately, this));

	this.bMenuInit =3D false;
};

BX.COpener.prototype.Load =3D function()
{
	if (this.PARAMS.MENU_URL && !this.bMenuLoaded)
	{
		if (!this.bMenuLoading)
		{
			var url =3D this.PARAMS.MENU_URL;
			if (url.indexOf('sessid=3D') <=3D 0)
				url +=3D (url.indexOf('?') > 0 ? '&' : '?') + 'sessid=3D' + BX.bitrix_s=
essid();

			this.bMenuLoading =3D true;
			BX.ajax.loadJSON(url, BX.proxy(this.SetMenu, this), BX.proxy(this.LoadFa=
iled, this));
		}
	}
};

BX.COpener.prototype.SetMenu =3D function(menu)
{
	this.bMenuLoaded =3D true;
	this.bMenuLoading =3D false;
	if (this.bMenuInit)
	{
		this.MENU.setItems(menu);
	}
	else
