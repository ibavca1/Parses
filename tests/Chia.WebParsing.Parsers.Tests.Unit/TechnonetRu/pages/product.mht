		arTags =3D ['b', 'em', 'font', 'h\\d', 'i', 'li', 'ol', 'p', 'small', 'sp=
an', 'strong', 'u', 'ul'],
		replaceEmptyTags =3D function(){i--; bReplasing =3D true; return ' ';},
		re, tagName, i, l;

