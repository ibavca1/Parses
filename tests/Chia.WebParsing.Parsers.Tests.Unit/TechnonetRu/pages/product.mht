	this.ToggleDelimiter =3D function(tab, on)
	{
		var d;
		if((d =3D jsUtils.FindNextSibling(tab, 'div')) && d.className.indexOf('vi=
ew-tab-delimiter') !=3D -1)
			d.className =3D 'view-tab-delimiter'+(on? '':' view-tab-hide-delimiter');
		if((d =3D jsUtils.FindPreviousSibling(tab, 'div')) && d.className.indexOf=
('view-tab-delimiter') !=3D -1)
			d.className =3D 'view-tab-delimiter'+(on? '':' view-tab-hide-delimiter');
	}

	this.DisableTab =3D function(tab_id)
	{
	}

	this.EnableTab =3D function(tab_id)
	{
	}

	this.ReplaceAnchor =3D function(tab)
	{
		var tab_div =3D document.getElementById('view_tab_'+tab["DIV"]);
		tab["HTML"] =3D tab_div.innerHTML;
		var a =3D jsUtils.FindChildObject(tab_div, "a");
		tab_div.innerHTML =3D a.innerHTML;
	}

	this.RebuildTabs =3D function()
	{
		var container =3D jsUtils.FindParentObject(document.getElementById('view_=
tab_'+_this.aTabs[0]["DIV"]), "div");
		var aPos =3D [0];
		var selectedIndex =3D -1;
		var prevTop =3D -1;
		var last;
		var n =3D container.childNodes.length;
		for(var i=3D0; i<n; i++)
		{
			var div =3D container.childNodes[i];
			if(!div.id)
				continue;

			if(prevTop > -1 && div.offsetTop > prevTop)
				aPos[aPos.length] =3D i;
			prevTop =3D div.offsetTop;

			if(selectedIndex =3D=3D -1 && div.className.indexOf('view-tab-active') !=
=3D -1)
				selectedIndex =3D aPos.length-1;
			last =3D div;
		}

		if(selectedIndex < aPos.length && selectedIndex > -1)
		{
			var aDiv =3D new Array();
			var div =3D container.childNodes[aPos[selectedIndex]];
			for(var i =3D aPos[selectedIndex]; i<aPos[selectedIndex+1]; i++)
			{
				aDiv[aDiv.length] =3D div;
				div =3D div.nextSibling;
			}
			if(aDiv.length > 0)
			{
				for(var i in aDiv)
					container.removeChild(aDiv[i]);

				while(last.nextSibling)
				{
					last =3D last.nextSibling;
					if(last.tagName && last.tagName.toUpperCase() =3D=3D 'BR' && last.clas=
sName && last.className =3D=3D 'tab-break')
						break;
				}

				var br =3D document.createElement("BR");
				br.style.clear=3D'both';
				container.insertBefore(br, last);

				for(var i in aDiv)
				{
					if(aDiv[i].tagName && aDiv[i].tagName.toUpperCase() =3D=3D 'BR')
						continue;
					container.insertBefore(aDiv[i], last);
				}
			}
		}
	}

	this.Init =3D function()
	{
		if(this.aTabs.length =3D=3D 0)
			return;
		for(var i in this.aTabs)
		{
			var div =3D document.getElementById(this.aTabs[i]["DIV"]);
			if(div.style.display !=3D 'none')
			{
				this.ReplaceAnchor(this.aTabs[i]);
				this.ToggleDelimiter(document.getElementById('view_tab_'+this.aTabs[i][=
"DIV"]), false);
				break;
			}
		}
		setTimeout(this.RebuildTabs, 10);
		window.onresize =3D this.RebuildTabs;
	}

	this.Init();
}

