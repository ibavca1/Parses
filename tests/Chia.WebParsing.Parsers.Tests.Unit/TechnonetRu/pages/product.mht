BX.CDialog.prototype.GetParameters =3D function(form_name)
{
	var form =3D this.GetForm();

	if(!form)
		return "";

	var i, s =3D "";
	var n =3D form.elements.length;

	var delim =3D '';
	for(i=3D0; i<n; i++)
	{
		if (s !=3D '') delim =3D '&';
		var el =3D form.elements[i];
		if (el.disabled)
			continue;

		switch(el.type.toLowerCase())
		{
			case 'text':
			case 'textarea':
			case 'password':
			case 'hidden':
				if (null =3D=3D form_name && el.name.substr(el.name.length-4) =3D=3D '_=
alt' && form.elements[el.name.substr(0, el.name.length-4)])
					break;
				s +=3D delim + el.name + '=3D' + BX.util.urlencode(el.value);
				break;
			case 'radio':
				if(el.checked)
					s +=3D delim + el.name + '=3D' + BX.util.urlencode(el.value);
				break;
			case 'checkbox':
				s +=3D delim + el.name + '=3D' + BX.util.urlencode(el.checked ? 'Y':'N'=
);
				break;
			case 'select-one':
				var val =3D "";
				if (null =3D=3D form_name && form.elements[el.name + '_alt'] && el.sele=
ctedIndex =3D=3D 0)
					val =3D form.elements[el.name+'_alt'].value;
				else
					val =3D el.value;
				s +=3D delim + el.name + '=3D' + BX.util.urlencode(val);
				break;
			case 'select-multiple':
				var j, bAdded =3D false;
				var l =3D el.options.length;
				for (j=3D0; j<l; j++)
				{
					if (el.options[j].selected)
					{
						s +=3D delim + el.name + '=3D' + BX.util.urlencode(el.options[j].valu=
e);
						bAdded =3D true;
					}
				}
				if (!bAdded)
					s +=3D delim + el.name + '=3D';
				break;
			default:
				break;
		}
	}

	return s;
};

