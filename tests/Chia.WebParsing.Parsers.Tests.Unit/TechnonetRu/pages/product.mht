		if (scripts[i].isInternal)
			scriptsInt +=3D ';' + scripts[i].JS
		else
			scriptsExt.push(scripts[i].JS);
	}

	scriptsExt =3D BX.util.array_unique(scriptsExt);

	var l=3Dl1=3DscriptsExt.length,
		f=3DscriptsInt.length>0?function(){BX.evalGlobal(scriptsInt)}:BX.DoNothin=
g;

	if(l>0)
	{
		var c=3Dfunction(){if(--l1<=3D0){f();cb();f=3DBX.DoNothing;}};
		for(var i=3D0; i<l;i++)
		{
			BX.loadScript(scriptsExt[i], c);
