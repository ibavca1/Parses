				if (nextNode)
				{
					while(nextNode =3D nextNode.nextSibling)
					{
						res =3D this.TestNodeWithAttributes(nextNode, nodeInfo);
						if (res)
							arFindedNodes.push(res);

						if (res.coincide =3D=3D 100)
							break;
					}
				}
			}

			// Find by next sibling
			if (nodeInfo.withId.nextSibling)
			{
				var prevNode =3D BX(nodeInfo.withId.nextSibling);
				if (prevNode)
				{
					while(prevNode =3D prevNode.previousSibling)
					{
						res =3D this.TestNodeWithAttributes(prevNode, nodeInfo);
						if (res)
							arFindedNodes.push(res);

						if (res.coincide =3D=3D 100)
							break;
					}
				}
			}

			// Find by child
			if (nodeInfo.withId.children)
			{
				var i, l =3D nodeInfo.withId.children.length, child, parNode;
				for (i =3D 0; i < l; i++)
				{
					child =3D BX(nodeInfo.withId.children[i]);
					if (child)
					{
						parNode =3D child;
						while (true)
						{
							parNode =3D BX.findParent(parNode, {tagName: nodeInfo.nodeName});
							if (!parNode)
								break;

							res =3D this.TestNodeWithAttributes(prevNode, nodeInfo);
							if (res)
								arFindedNodes.push(res);

							if (res.coincide =3D=3D 100)
								break;
						}
					}
				}
			}

			// Find by parent
			var parent;
			if (nodeInfo.withId.parent)
				parent =3D BX(nodeInfo.withId.parent);
			if (!parent)
				parent =3D document.body;

			var arAllNodes =3D parent.getElementsByTagName(nodeInfo.nodeName);
			var i, l =3D arAllNodes.length;
			for (i =3D 0; i < l; i++)
