				if (res.coincide =3D=3D 100)
					break;
			}
		}
		else
		{
			arFindedNodes.push({coincide: 100, node: node, bImpAttrCoincide: true});
		}

		var i, l =3D arFindedNodes.length;
		var arRealNodes =3D [], maxCoincide =3D 0, mostRealNode =3D false;

		for (i =3D 0; i < l; i++)
		{
			if (arFindedNodes[i].coincide > maxCoincide)
			{
				maxCoincide =3D arFindedNodes[i].coincide;
				mostRealNode =3D arFindedNodes[i].node;
				arRealNodes =3D [];
			}

			if (arFindedNodes[i].coincide =3D=3D maxCoincide && arFindedNodes[i].nod=
e !=3D mostRealNode)
				arRealNodes.push(arFindedNodes[i].node);
		}

		if (arRealNodes.length =3D=3D 0 && mostRealNode)
			return mostRealNode;
		else
			arRealNodes[0];

		return false;
