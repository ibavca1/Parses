			re =3D new RegExp('<'+tagName+'[^>]*?>\\s*?</'+tagName+'>', 'ig');
			str =3D str.replace(re, replaceEmptyTags);

			re =3D new RegExp('<' + tagName + '\\s+?[^>]*?/>', 'ig');
			str =3D str.replace(re, replaceEmptyTags);

