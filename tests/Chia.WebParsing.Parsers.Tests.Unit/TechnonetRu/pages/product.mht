function JCHttpRequest()
{
	this.Action =3D null; //function(result){}

	this._OnDataReady =3D function(result)
	{
		if(this.Action)
			this.Action(result);
	}

	this._CreateHttpObject =3D function()
	{
		var obj =3D null;
		if(window.XMLHttpRequest)
		{
			try {obj =3D new XMLHttpRequest();} catch(e){}
		}
        else if(window.ActiveXObject)
        {
            try {obj =3D new ActiveXObject("Microsoft.XMLHTTP");} catch(e){}
            if(!obj)
            	try {obj =3D new ActiveXObject("Msxml2.XMLHTTP");} catch (e){}
        }
        return obj;
	}

	this._SetHandler =3D function(httpRequest)
	{
		var _this =3D this;
		httpRequest.onreadystatechange =3D function()
		{
			if(httpRequest.readyState =3D=3D 4)
			{
//				try
				{
					var s =3D httpRequest.responseText;
					var code =3D [];
					var start, end;
					while((start =3D s.indexOf('<script>')) !=3D -1)
					{
						var end =3D s.indexOf('</script>', start);
						if(end =3D=3D -1)
							break;

						code[code.length] =3D s.substr(start+8, end-start-8);
						s =3D s.substr(0, start) + s.substr(end+9);
					}
					_this._OnDataReady(s);

					for(var i =3D 0, cnt =3D code.length; i < cnt; i++)
						if(code[i] !=3D '')
							jsUtils.EvalGlobal(code[i]);
				}
/*
				catch (e)
				{
					var w =3D window.open("about:blank");
					w.document.write(httpRequest.responseText);
					w.document.close();
				}
*/
			}
		}
	}

	this.Send =3D function(url)
	{
		var httpRequest =3D this._CreateHttpObject();
		if(httpRequest)
		{
			httpRequest.open("GET", url, true);
			this._SetHandler(httpRequest);
			return httpRequest.send("");
  		}
	}

	this.Post =3D function(url, data)
	{
		var httpRequest =3D this._CreateHttpObject();
		if(httpRequest)
		{
			httpRequest.open("POST", url, true);
			this._SetHandler(httpRequest);
			httpRequest.setRequestHeader('Content-Type', 'application/x-www-form-url=
encoded');
			return httpRequest.send(data);
  		}
	}
}
