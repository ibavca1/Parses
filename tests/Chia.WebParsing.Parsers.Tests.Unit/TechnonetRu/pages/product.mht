	TestNodeWithAttributes: function(pNode, nodeInfo)
	{
		if (!pNode || !pNode.nodeName)
			return false;

		var res =3D {coincide: 0, node: pNode};
		var info =3D this._GetNodeAdjustInfo(pNode);

		if (info.nodeName !=3D nodeInfo.nodeName)
			return false;

		var delta =3D 0;
		var bInnerHTML =3D typeof nodeInfo.innerHTML =3D=3D 'string';
		if (typeof info.innerHTML !=3D 'string' && bInnerHTML)
			return false;

		var count =3D 0;
		for (i in nodeInfo.attr)
			if (typeof nodeInfo.attr[i] =3D=3D 'string')
				count++;

		if (count > 0)
		{
