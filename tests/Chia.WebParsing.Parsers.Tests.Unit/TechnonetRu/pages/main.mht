	text =3D text.replace(/(<td[\s\S]*?)valign=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)nowrap=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)nowrap([\s\S]*?>)/gi, "$1$3");

