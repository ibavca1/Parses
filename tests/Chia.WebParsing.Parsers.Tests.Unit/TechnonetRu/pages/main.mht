	text =3D text.replace(/<link[^>]*?(?:>\s*?<\/link)?(?:\/?)?>/ig, '');
	text =3D text.replace(/<style[\s\S]*?<\/style>/ig, '');

	text =3D text.replace(/<table([\s\S]*?)>/gi, "<table>");
	text =3D text.replace(/<tr([\s\S]*?)>/gi, "<tr>");
	text =3D text.replace(/(<td[\s\S]*?)width=3D("|')[\s\S]*?\2([\s\S]*?>)/gi,=
 "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)height=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)style=3D("|')[\s\S]*?\2([\s\S]*?>)/gi,=
