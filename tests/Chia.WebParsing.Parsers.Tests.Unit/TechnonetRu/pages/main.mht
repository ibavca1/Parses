				this.ToggleDelimiter(document.getElementById('view_tab_'+this.aTabs[i][=
"DIV"]), false);
				break;
			}
		}
		setTimeout(this.RebuildTabs, 10);
		window.onresize =3D this.RebuildTabs;
	}

	this.Init();
