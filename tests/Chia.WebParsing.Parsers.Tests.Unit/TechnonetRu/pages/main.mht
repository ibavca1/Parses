				div =3D div.nextSibling;
			}
			if(aDiv.length > 0)
			{
				for(var i in aDiv)
					container.removeChild(aDiv[i]);

				while(last.nextSibling)
				{
					last =3D last.nextSibling;
					if(last.tagName && last.tagName.toUpperCase() =3D=3D 'BR' && last.clas=
sName && last.className =3D=3D 'tab-break')
						break;
				}

				var br =3D document.createElement("BR");
				br.style.clear=3D'both';
				container.insertBefore(br, last);

				for(var i in aDiv)
				{
					if(aDiv[i].tagName && aDiv[i].tagName.toUpperCase() =3D=3D 'BR')
						continue;
					container.insertBefore(aDiv[i], last);
				}
			}
		}
	}

	this.Init =3D function()
	{
		if(this.aTabs.length =3D=3D 0)
			return;
		for(var i in this.aTabs)
