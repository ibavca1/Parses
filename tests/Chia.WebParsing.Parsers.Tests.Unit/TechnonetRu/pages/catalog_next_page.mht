	text =3D text.replace(/<xml[^>]*?(?:>\s*?<\/xml)?(?:\/?)?>/ig, '');
	text =3D text.replace(/<meta[^>]*?(?:>\s*?<\/meta)?(?:\/?)?>/ig, '');
	text =3D text.replace(/<link[^>]*?(?:>\s*?<\/link)?(?:\/?)?>/ig, '');
	text =3D text.replace(/<style[\s\S]*?<\/style>/ig, '');

	text =3D text.replace(/<table([\s\S]*?)>/gi, "<table>");
	text =3D text.replace(/<tr([\s\S]*?)>/gi, "<tr>");
	text =3D text.replace(/(<td[\s\S]*?)width=3D("|')[\s\S]*?\2([\s\S]*?>)/gi,=
 "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)height=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)style=3D("|')[\s\S]*?\2([\s\S]*?>)/gi,=
 "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)valign=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)nowrap=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<td[\s\S]*?)nowrap([\s\S]*?>)/gi, "$1$3");

	text =3D text.replace(/(<col[\s\S]*?)width=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");
	text =3D text.replace(/(<col[\s\S]*?)style=3D("|')[\s\S]*?\2([\s\S]*?>)/gi=
, "$1$3");

	// For Opera (12.10+) only when in text we have reference links.
	if (BX.browser.IsOpera())
		text =3D text.replace(/REF\s+?_Ref\d+?[\s\S]*?MERGEFORMAT\s([\s\S]*?)\s[\=
s\S]*?<\/xml>/gi, " $1 ");

