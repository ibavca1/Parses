	while((pElement1 =3D pElement1.parentNode) !=3D null)
		arr1[arr1.length] =3D pElement1;
	while((pElement2 =3D pElement2.parentNode) !=3D null)
		arr2[arr2.length] =3D pElement2;

	var min, diff1 =3D 0, diff2 =3D 0;
	if(arr1.length<arr2.length)
	{
		min =3D arr1.length;
		diff2 =3D arr2.length - min;
