	// Remove mso-xxx styles.
	text =3D text.replace(/\s*mso-[^:]+:[^;"]+;?/gi, "");

	// Remove margin styles.
	text =3D text.replace(/\s*margin: 0cm 0cm 0pt\s*;/gi, "");
	text =3D text.replace(/\s*margin: 0cm 0cm 0pt\s*"/gi, "\"");

	text =3D text.replace(/\s*TEXT-INDENT: 0cm\s*;/gi, "");
