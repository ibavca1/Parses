/****************************************/
/* RESET CSS */
/****************************************/

#jivo_top_wrap a,
#jivo_top_wrap img {
	background: transparent!important;
	margin:0px;
	padding: 0px;
	float: none!important;
	webkit-transition:none;
	-moz-transition:none;
	-o-transition:none;
	-ms-transition:none;
	transition:none;
	width: auto;
	height: auto;
	border: 0px;
}

#jivo_top_wrap table {
	border-width: 0px;
	width: auto;
}


#jivo_top_wrap div {
	margin: 0px;
    text-indent: 0px;
}

/****************************************/
/* GLOBAL STUFF */
/****************************************/

#jivo_top_wrap, .jivo_top_wrap {
	position: fixed;
	right: 10px;
	bottom: 0px;
	height: auto;
	z-index: 300000;
	background-color: transparent;
	margin: 0;
	width: 262px;
}

.jivo_align_left {
	left: 10px;
}

.jivo_align_right {
	right: 10px;
}

body#outer-body {
	margin: 0;
	padding: 0 40px 0 0;
	left: 0;
	top: 0;
	overflow: hidden!important;
	z-index: 0;
}

/****************************************/
/* IFRAME STUFF */
/****************************************/

#jivo_magic_iframe {
	width: 100%;
	height: 100%;
	position: fixed;
	margin: 0;
	padding: 0;
	left: 0;
	top: 0;
	border: 0;
	z-index: 200000;
	background-color: white;
	-webkit-overflow-scrolling:touch; /*css trick for iOs devices*/
	overflow: auto;
	/* tim: fix for rsb.ru */
	filter: none;=20
}

.frm_viz {
	display: block;
	visibility : visible;
}

.frm_inviz{
	display:block;
	visibility : hidden;
	width: 100%;
	height: 100%;
	position: absolute;
	left: -10000px;
	top: -10000px;
}

/****************************************/
/* WIDGET */
/****************************************/

#jivo_chat_widget {
	font-family: Arial, sans-serif;
	font-weight: bold;
	font-size: 12px;
	color: #464646;
	z-index: 300000;
	cursor: default;
	cursor: pointer;
}

#jivo_chat_widget_left {
	width: 14px;
	height: 14px;
	margin: 8px 3px 0 10px;
}

#jivo_chat_widget_center {
	min-width: 140px;
	font-size: 12px;
	height: 100%;
	padding: 0px 0px 0 4px;
	line-height: 29px;
	/* IE 8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=3D80)";
	/* IE 5-7 */
	filter: alpha(opacity=3D80);
	/* Netscape */
	-moz-opacity: 0.8;
	/* Safari 1.x */
	-khtml-opacity: 0.8;
	/* Good browsers */
	opacity: 0.8;
}

#jivo_chat_widget_center:hover {
	/* IE 8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=3D100)";
	/* IE 5-7 */
	filter: alpha(opacity=3D100);
	/* Netscape */
	-moz-opacity: 1;
	/* Safari 1.x */
	-khtml-opacity: 1;
	/* Good browsers */
	opacity: 1;
}

#jivo_chat_widget_center:active {
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=3D80)";
	/* IE 5-7 */
	filter: alpha(opacity=3D50);
	/* Netscape */
	-moz-opacity: 0.5;
	/* Safari 1.x */
	-khtml-opacity: 0.5;
	/* Good browsers */
	opacity: 0.5;
}

#jivo_chat_widget_offline #jivo_chat_widget_center {
	padding: 0px 10px 0 15px;
}

#jivo_chat_widget_right {
	width: 50px;
