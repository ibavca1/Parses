	text =3D text.replace(/<(?:[^\s>]+)[^>]*>([\s\n\t\r]*)<\/\1>/g, "$1");
	text =3D text.replace(/<(?:[^\s>]+)[^>]*>(\s*)<\/\1>/g, "$1");
	text =3D text.replace(/<(?:[^\s>]+)[^>]*>(\s*)<\/\1>/g, "$1");

