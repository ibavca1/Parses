CleanWordText: function(text)
{
	text =3D text.replace(/<(P|B|U|I|STRIKE)>&nbsp;<\/\1>/g, ' ');
	text =3D text.replace(/<o:p>([\s\S]*?)<\/o:p>/ig, "$1");
