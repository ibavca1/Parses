function JCStartMenu()
{
	var menuStart =3D null;
	var request =3D new JCHttpRequest();
	var _this =3D this;

	this.EvalMenu =3D function(result)
	{
		if(jsUtils.trim(result).length =3D=3D 0)
			return;

		var menuItems;
        try
        {
		    eval(result); // menuItems=3D{'styles':[], 'items':[]}
        }
        catch(e)
        {
        }

		if(!menuItems)
			return false;

		//Applying styles
		var head =3D document.getElementsByTagName("HEAD");
		if(head && head[0])
		{
			var style =3D document.createElement("STYLE");
			head[0].appendChild(style);
			if(jsUtils.IsIE())
				document.styleSheets[document.styleSheets.length-1].cssText =3D menuIte=
ms['styles'];
			else
				style.appendChild(document.createTextNode(menuItems['styles']));
		}
		return menuItems;
	}

	this.ShowStartMenu =3D function(button, back_url)
	{
		var dPos =3D {'left':0, 'top':0, 'right':0, 'bottom':0};
		if(!menuStart || !menuStart.menuItems)
		{
			request.Action =3D function(result)
			{
				var menuItems =3D _this.EvalMenu(result);
				if(menuItems)
				{
					//show menu
					menuStart.PopupHide();
					menuStart.ShowMenu(button, menuItems['items'], jsPanel.IsFixed(), dPos=
);
				}
			}
			//create menu
			menuStart =3D new PopupMenu('panel_start_menu');
			menuStart.Create(1100);
			menuStart.ShowMenu(button, [{
				'TEXT':phpVars.messMenuLoading,
				'TITLE':phpVars.messMenuLoadingTitle,
				'ICONCLASS':'loading',
				'AUTOHIDE':false}], jsPanel.IsFixed(), dPos);
			request.Send('/bitrix/admin/get_start_menu.php?lang=3D'+phpVars.LANGUAGE=
_ID+(back_url? '&back_url_pub=3D'+encodeURIComponent(back_url):'')+'&sessid=
=3D'+phpVars.bitrix_sessid);
		}
		else
		{
			menuStart.ShowMenu(button, null, jsPanel.IsFixed(), dPos);
		}
	}

	this.PreloadMenu =3D function(back_url)
	{
		if(!menuStart)
		{
			request.Action =3D function(result)
			{
				var menuItems =3D _this.EvalMenu(result);
				if(menuItems)
				{
					//show menu
					menuStart.SetItems(menuItems['items']);
					menuStart.BuildItems();
				}
			}
			//create menu
			menuStart =3D new PopupMenu('panel_start_menu');
			menuStart.Create(1100);
			request.Send('/bitrix/admin/get_start_menu.php?lang=3D'+phpVars.LANGUAGE=
_ID+(back_url? '&back_url_pub=3D'+encodeURIComponent(back_url):'')+'&sessid=
=3D'+phpVars.bitrix_sessid);
		}
	}

	this.OpenDynMenu =3D function(menu, module_id, items_id, back_url)
	{
		request.Action =3D function(result)
		{
			if(jsUtils.trim(result).length =3D=3D 0)
				return;

			var menuItems;
			eval(result); // menuItems=3D{'items':[]}

			if(menu && menuItems)
			{
				var bVisible =3D menu.IsVisible();
				menu.PopupHide();
				menu.SetItems(menuItems['items']);
				menu.BuildItems();
				menu.parentMenu.ShowSubmenu(menu.parentItem, false, !bVisible);
			}
		}
		request.Send('/bitrix/admin/get_start_menu.php?mode=3Ddynamic&lang=3D'+ph=
pVars.LANGUAGE_ID+'&admin_mnu_module_id=3D'+encodeURIComponent(module_id)+'=
&admin_mnu_menu_id=3D'+encodeURIComponent(items_id)+(back_url? '&back_url_p=
ub=3D'+encodeURIComponent(back_url):'')+'&sessid=3D'+phpVars.bitrix_sessid);
	}

	this.OpenURL =3D function(item, arguments, url, back_url)
	{
		var itemInfo =3D menuStart.GetItemInfo(item);
		if(itemInfo)
		{
			request.Action =3D function(result){}
			request.Send('/bitrix/admin/get_start_menu.php?mode=3Dsave_recent&url=3D=
'+encodeURIComponent(url)+'&text=3D'+encodeURIComponent(itemInfo['TEXT'])+'=
&title=3D'+encodeURIComponent(itemInfo['TITLE'])+'&icon=3D'+itemInfo['ICON'=
]+'&sessid=3D'+phpVars.bitrix_sessid);
		}
		if(back_url)
			url +=3D (url.indexOf('?')>=3D0? '&':'?')+'back_url_pub=3D'+encodeURICom=
ponent(back_url);
		jsUtils.Redirect(arguments, url);
	}
}
