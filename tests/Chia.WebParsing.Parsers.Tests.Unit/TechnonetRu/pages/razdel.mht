	if (this.freeze)
		return;

	var _this =3D this;
	var oDiv =3D document.body.appendChild(document.createElement("DIV"));
	oDiv.onmouseover =3D function(){_this.oDivOver =3D true};
	oDiv.onmouseout =3D function(){_this.oDivOver =3D false; _this.SmartHide(_=
this);}
	oDiv.id =3D "__BXHint_div";
