		oDiv.style.width =3D width + "px";

	if (height)
		oDiv.style.height =3D height + "px";
	oDiv.innerHTML =3D innerHTML;

	var w =3D oDiv.offsetWidth;
	var h =3D oDiv.offsetHeight;
	if (this.bWidth)
	{
		if (!width && w>200)
			w =3D Math.round(Math.sqrt(1.618*w*h));
		oDiv.style.width =3D w + "px";
		h =3D oDiv.offsetHeight;
	}

	var pos =3D {left : this.x + 10, right : this.x + w, top : this.y, bottom =
: this.y + h};

	pos =3D this.AlignToPos(pos, w, h);

	oDiv.style.zIndex =3D 2100;

	jsFloatDiv.Show(oDiv, pos.left, pos.top,3);

//	oDiv.ondrag =3D jsUtils.False;
//	oDiv.onselectstart =3D jsUtils.False;
//	oDiv.style.MozUserSelect =3D 'none';
	oDiv =3D null;
}

BXHint.prototype.AlignToPos =3D function(pos, w, h)
{
	var body =3D document.body;
	if((body.clientWidth + body.scrollLeft) < (pos.left + w))
		pos.left =3D (pos.left - w >=3D 0) ? (pos.left - w) : body.scrollLeft;

	if((body.clientHeight + body.scrollTop) - (pos["bottom"]) < 0)
		pos.top =3D (pos.top - h >=3D 0) ? (pos.top - h) : body.scrollTop;

	return pos;
}

