	text =3D text.replace(/<span[^>]*display:\s*?none[^>]*>([\s\S]*?)<\/span>/=
gi, ''); // Hide spans with display none

	text =3D text.replace(/<!--\[[\s\S]*?\]-->/ig, ""); //<!--[.....]--> <!--[=
if gte mso 9]>...<![endif]-->
	text =3D text.replace(/<!\[[\s\S]*?\]>/ig, ""); //	<! [if !vml]>
	text =3D text.replace(/<\\?\?xml[^>]*>/ig, ""); //<xml...>, </xml...>

	text =3D text.replace(/<o:p>\s*<\/o:p>/ig, "");

	text =3D text.replace(/<\/?[a-z1-9]+:[^>]*>/gi, "");	//<o:p...>, </o:p>
	text =3D text.replace(/<([a-z1-9]+[^>]*) class=3D([^ |>]*)(.*?>)/gi, "<$1$=
3");
