	text =3D text.replace(/<FONT[^>]*>([\s\S]*?)<\/FONT>/gi, '$1');
	text =3D text.replace(/\s*face=3D"[^"]*"/gi, "");
	text =3D text.replace(/\s*face=3D[^ >]*/gi, "");
	text =3D text.replace(/\s*FONT-FAMILY:[^;"]*;?/gi, "");

	// Remove Class attributes
	text =3D text.replace(/<(\w[^>]*) class=3D([^ |>]*)([^>]*)/gi, "<$1$3");

	// Remove styles.
	text =3D text.replace(/<(\w[^>]*) style=3D"([^\"]*)"([^>]*)/gi, "<$1$3");

	// Remove empty styles.
	text =3D text.replace(/\s*style=3D"\s*"/gi, '');

	// Remove Lang attributes
	text =3D text.replace(/<(\w[^>]*) lang=3D([^ |>]*)([^>]*)/gi, "<$1$3");

	var iter =3D 0;
	while (text.toLowerCase().indexOf('<span') !=3D -1 && text.toLowerCase().i=
ndexOf('</span>') !=3D -1 && iter++ < 20)
		text =3D text.replace(/<span[^>]*?>([\s\S]*?)<\/span>/gi, '$1');

	var
		_text,
		i, tag, arFormatTags =3D ['b', 'strong', 'i', 'u', 'font', 'span', 'strik=
e'];

