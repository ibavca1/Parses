	padding: 0 40px 0 0;
	left: 0;
	top: 0;
	overflow: hidden!important;
	z-index: 0;
}

/****************************************/
/* IFRAME STUFF */
/****************************************/

#jivo_magic_iframe {
	width: 100%;
	height: 100%;
	position: fixed;
	margin: 0;
	padding: 0;
	left: 0;
	top: 0;
	border: 0;
	z-index: 200000;
	background-color: white;
	-webkit-overflow-scrolling:touch; /*css trick for iOs devices*/
	overflow: auto;
	/* tim: fix for rsb.ru */
	filter: none;=20
}

.frm_viz {
	display: block;
	visibility : visible;
}

.frm_inviz{
	display:block;
	visibility : hidden;
	width: 100%;
	height: 100%;
	position: absolute;
	left: -10000px;
	top: -10000px;
}

