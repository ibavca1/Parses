			}
		}

		var active_tab =3D document.getElementById('view_tab_'+tab_id);
		active_tab.className =3D 'view-tab view-tab-active';
		this.ToggleDelimiter(active_tab, false);
		div.style.display =3D 'block';

		this.RebuildTabs();

		for(var i in this.aTabs)
		{
			if(this.aTabs[i]["DIV"] =3D=3D tab_id)
			{
				this.ReplaceAnchor(this.aTabs[i]);
