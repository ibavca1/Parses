/****************************************/
/* RESET CSS */
/****************************************/

#jivo_top_wrap a,
#jivo_top_wrap img {
	background: transparent!important;
	margin:0px;
	padding: 0px;
	float: none!important;
	webkit-transition:none;
	-moz-transition:none;
	-o-transition:none;
	-ms-transition:none;
	transition:none;
	width: auto;
	height: auto;
	border: 0px;
}

#jivo_top_wrap table {
	border-width: 0px;
	width: auto;
}


#jivo_top_wrap div {
	margin: 0px;
    text-indent: 0px;
}

/****************************************/
/* GLOBAL STUFF */
/****************************************/

#jivo_top_wrap, .jivo_top_wrap {
	position: fixed;
	right: 10px;
	bottom: 0px;
	height: auto;
	z-index: 300000;
	background-color: transparent;
	margin: 0;
	width: 262px;
}

.jivo_align_left {
	left: 10px;
}

.jivo_align_right {
	right: 10px;
}

body#outer-body {
