	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=3D80)";
	/* IE 5-7 */
	filter: alpha(opacity=3D80);
	/* Netscape */
	-moz-opacity: 0.8;
	/* Safari 1.x */
	-khtml-opacity: 0.8;
	/* Good browsers */
	opacity: 0.8;
