				||
				(!e.ctrlKey && e.shiftKey && !e.altKey && e.keyCode =3D=3D 45) /*Shift+=
Ins*/
				||
				(e.metaKey && !e.shiftKey && !e.altKey && e.keyCode =3D=3D 86) /* Cmd+V=
