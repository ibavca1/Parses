BX.getNumMonth =3D function(month)
{
	var wordMonthCut =3D ['jan', 'feb', 'mar', 'apr', 'may', 'jun', 'jul', 'au=
g', 'sep', 'oct', 'nov', 'dec'];
	var wordMonth =3D ['january', 'february', 'march', 'april', 'may', 'june',=
 'july', 'august', 'september', 'october', 'november', 'december'];

	var q =3D month.toUpperCase();
	for (i =3D 1; i <=3D 12; i++)
	{
		if (q =3D=3D BX.message('MON_'+i).toUpperCase() || q =3D=3D BX.message('M=
ONTH_'+i).toUpperCase() || q =3D=3D wordMonthCut[i-1].toUpperCase() || q =
=3D=3D wordMonth[i-1].toUpperCase())
