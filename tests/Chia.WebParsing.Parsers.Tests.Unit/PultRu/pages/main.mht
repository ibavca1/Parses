BX.parseDate =3D function(str)
{
	if (BX.type.isNotEmptyString(str))
	{
		var regMonths =3D '';
		for (i =3D 1; i <=3D 12; i++)
		{
			regMonths =3D regMonths + '|' + BX.message('MON_'+i);
		}

		var expr =3D new RegExp('([0-9]+|[a-z]+' + regMonths + ')', 'ig');
		var aDate =3D str.match(expr),
			aFormat =3D BX.message('FORMAT_DATE').match(/(DD|MI|MMMM|MM|M|YYYY)/ig),
			i, cnt,
			aDateArgs=3D[], aFormatArgs=3D[],
			aResult=3D{};

		if (!aDate)
			return null;

		if(aDate.length > aFormat.length)
		{
			aFormat =3D BX.message('FORMAT_DATETIME').match(/(DD|MI|MMMM|MM|M|YYYY|H=
H|H|SS|TT|T|GG|G)/ig);
		}

		for(i =3D 0, cnt =3D aDate.length; i < cnt; i++)
		{
			if(BX.util.trim(aDate[i]) !=3D '')
			{
				aDateArgs[aDateArgs.length] =3D aDate[i];
			}
		}

		for(i =3D 0, cnt =3D aFormat.length; i < cnt; i++)
		{
			if(BX.util.trim(aFormat[i]) !=3D '')
			{
				aFormatArgs[aFormatArgs.length] =3D aFormat[i];
			}
		}


		var m =3D BX.util.array_search('MMMM', aFormatArgs)
		if (m > 0)
		{
			aDateArgs[m] =3D BX.getNumMonth(aDateArgs[m]);
			aFormatArgs[m] =3D "MM";
		}
		else
		{
			m =3D BX.util.array_search('M', aFormatArgs)
			if (m > 0)
			{
				aDateArgs[m] =3D BX.getNumMonth(aDateArgs[m]);
				aFormatArgs[m] =3D "MM";
			}
		}

		for(i =3D 0, cnt =3D aFormatArgs.length; i < cnt; i++)
		{
			var k =3D aFormatArgs[i].toUpperCase();
			aResult[k] =3D k =3D=3D 'T' || k =3D=3D 'TT' ? aDateArgs[i] : parseInt(a=
DateArgs[i], 10);
		}

		if(aResult['DD'] > 0 && aResult['MM'] > 0 && aResult['YYYY'] > 0)
		{
			var d =3D new Date();
			d.setDate(1);
			d.setFullYear(aResult['YYYY']);
			d.setMonth(aResult['MM']-1);
			d.setDate(aResult['DD']);
			d.setHours(0, 0, 0);

			if(
				(!isNaN(aResult['HH']) || !isNaN(aResult['GG']) || !isNaN(aResult['H'])=
 || !isNaN(aResult['G']))
					&& !isNaN(aResult['MI'])
			)
			{
				if (!isNaN(aResult['H']) || !isNaN(aResult['G']))
				{
					var bPM =3D (aResult['T']||aResult['TT']||'am').toUpperCase()=3D=3D'PM=
';
					aResult['HH'] =3D parseInt(aResult['H']||aResult['G']||0, 10) + (bPM ?=
 12 : 0);
				}
				else
				{
					aResult['HH'] =3D parseInt(aResult['HH']||aResult['GG']||0, 10);
				}

				if (isNaN(aResult['SS']))
					aResult['SS'] =3D 0;

				d.setHours(aResult['HH'], aResult['MI'], aResult['SS']);
			}

			return d;
		}
	}

	return null;
};

