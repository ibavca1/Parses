					config.xhr.onreadystatechange =3D BX.DoNothing;

					// var status =3D oAjax.arThreads[TID].httpRequest.getResponseHeader('=
X-Bitrix-Ajax-Status');
					// var bRedirect =3D (status =3D=3D 'Redirect');

					if (status =3D=3D 'success')
					{
						var data =3D config.xhr.responseText;

						if (config.lsId)
						{
							BX.localStorage.set('ajax-' + config.lsId, data, config.lsTimeout);
						}

