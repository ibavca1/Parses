		if (v !=3D=3D null)
		{
			bXHR =3D false;

			var lsHandler =3D function(lsData) {
				if (lsData.key =3D=3D 'ajax-' + config.lsId && lsData.value !=3D 'BXAJA=
XWAIT')
				{
					var data =3D lsData.value,
						bRemove =3D !!lsData.oldValue && data =3D=3D null;
					if (!bRemove)
						BX.ajax.__run(config, data);
					else if (config.onfailure)
						config.onfailure("timeout");

					BX.removeCustomEvent('onLocalStorageChange', lsHandler);
				}
			}

			if (v =3D=3D 'BXAJAXWAIT')
			{
