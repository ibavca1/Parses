		BX.ready(function() {
			BX.loadScript.apply(this, _args);
		});
		return;
	}

	doc =3D doc || document;

	if (BX.type.isString(script))
		script =3D [script];
	var _callback =3D function()
	{
		return (callback && BX.type.isFunction(callback)) ? callback() : null
	}
	var load_js =3D function(ind)
	{
		if(ind >=3D script.length)
			return _callback();

		if(!!script[ind])
		{
			var fileSrc =3D BX.getJSPath(script[ind]);
			if(_isScriptLoaded(fileSrc))
			{
				load_js(++ind);
			}
			else
			{
				var oHead =3D doc.getElementsByTagName("head")[0] || doc.documentElemen=
t;
				var oScript =3D doc.createElement('script');
				oScript.src =3D script[ind];

				var bLoaded =3D false;
				oScript.onload =3D oScript.onreadystatechange =3D function()
				{
					if (!bLoaded && (!oScript.readyState || oScript.readyState =3D=3D "loa=
ded" || oScript.readyState =3D=3D "complete"))
					{
						bLoaded =3D true;
						setTimeout(function (){load_js(++ind);}, 50);

						oScript.onload =3D oScript.onreadystatechange =3D null;
						if (oHead && oScript.parentNode)
						{
							oHead.removeChild(oScript);
						}
					}
				}

				jsList.push(fileSrc);
				return oHead.insertBefore(oScript, oHead.firstChild);
			}
		}
		else
		{
			load_js(++ind);
		}
	}

	load_js(0);
};

BX.loadCSS =3D function(arCSS, doc, win)
{
	if (!BX.isReady)
	{
		var _args =3D arguments;
		BX.ready(function() {
			BX.loadCSS.apply(this, _args);
		});
		return null;
	}

	var bSingle =3D false;
	if (BX.type.isString(arCSS))
	{
		bSingle =3D true;
		arCSS =3D [arCSS];
	}

	var i =3D 0,
		l =3D arCSS.length,
		lnk =3D null,
		pLnk =3D [];

	if (l =3D=3D 0)
		return null;

	doc =3D doc || document;
	win =3D win || window;

	if (!win.bxhead)
	{
		var heads =3D doc.getElementsByTagName('HEAD');
		win.bxhead =3D heads[0];

		if (!win.bxhead)
		{
			return null;
		}
	}

	for (i =3D 0; i < l; i++)
	{
		var _check =3D BX.getCSSPath(arCSS[i]);
		if (_isCssLoaded(_check))
		{
			continue;
		}

		lnk =3D document.createElement('LINK');
		lnk.href =3D arCSS[i];
		lnk.rel =3D 'stylesheet';
		lnk.type =3D 'text/css';

		var templateLink =3D _getTemplateLink(win.bxhead);
		if (templateLink !=3D=3D null)
		{
			templateLink.parentNode.insertBefore(lnk, templateLink);
		}
		else
		{
			win.bxhead.appendChild(lnk);
		}

		pLnk.push(lnk);
		cssList.push(_check);
	}

	if (bSingle)
		return lnk;

	return pLnk;
};

