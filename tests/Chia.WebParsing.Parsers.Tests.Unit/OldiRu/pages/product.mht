				continue;
			}

			var elBorderLeftWidth =3D parseInt(BX.style(el, 'border-left-width')),
				elBorderTopWidth =3D parseInt(BX.style(el, 'border-top-width'));

			if (!isNaN(elBorderLeftWidth) && elBorderLeftWidth > 0)
