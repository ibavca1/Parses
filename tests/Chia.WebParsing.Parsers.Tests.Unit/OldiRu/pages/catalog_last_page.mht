	var data =3D '';
	if (BX.type.isString(arData))
		data =3D arData;
	else if (null !=3D arData)
	{
		for(var i in arData)
		{
			if (arData.hasOwnProperty(i))
			{
				if (data.length > 0)
					data +=3D '&';
				var name =3D BX.util.urlencode(i);
				if(prefix)
					name =3D prefix + '[' + name + ']';
				if(typeof arData[i] =3D=3D 'object')
					data +=3D BX.ajax.prepareData(arData[i], name);
				else
					data +=3D name + '=3D' + BX.util.urlencode(arData[i]);
			}
		}
	}
	return data;
};

