/*
arObs =3D [{
	url: url,
	type: html|script|json|css,
	callback: function
}]
*/
BX.ajax.load =3D function(arObs, callback)
{
	if (!BX.type.isArray(arObs))
		arObs =3D [arObs];

	var cnt =3D 0;

	if (!BX.type.isFunction(callback))
		callback =3D BX.DoNothing;

	var handler =3D function(data)
		{
			if (BX.type.isFunction(this.callback))
				this.callback(data);

			if (++cnt >=3D len)
				callback();
		};

	for (var i =3D 0, len =3D arObs.length; i<len; i++)
	{
		switch(arObs.type.toUpperCase())
		{
			case 'SCRIPT':
				BX.loadScript([arObs[i].url], BX.proxy(handler, arObs[i]));
			break;
			case 'CSS':
				BX.loadCSS([arObs[i].url]);

				if (++cnt >=3D len)
					callback();
			break;
			case 'JSON':
				BX.ajax.loadJSON(arObs.url, BX.proxy(handler, arObs[i]));
			break;

			default:
				BX.ajax.get(arObs.url, '', BX.proxy(handler, arObs[i]));
			break;
		}
	}
};

