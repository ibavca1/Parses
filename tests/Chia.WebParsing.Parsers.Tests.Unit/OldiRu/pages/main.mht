// function to format a number with separators. returns formatted number.
// n - the number to be formatted
// d - the decimal point character. usually: "."
// s - the separator character. if skipped, " " is used
function numFormat(n, d, s) {
    if (arguments.length =3D=3D 2) { s =3D " "; }
    if (arguments.length =3D=3D 1) { s =3D " "; d =3D "."; }
    n =3D n.toString();
    a =3D n.split(d);
    x =3D a[0];
    y =3D a[1];
    z =3D "";
    if (typeof(x) !=3D "undefined") {

    for (i=3Dx.length-1;i>=3D0;i--)
        z +=3D x.charAt(i);

    z =3D z.replace(/(\d{3})/g, "$1" + s);
    if (z.slice(-s.length) =3D=3D s)
        z =3D z.slice(0, -s.length);
    x =3D "";
    for (i=3Dz.length-1;i>=3D0;i--)
        x +=3D z.charAt(i);
    if (typeof(y) !=3D "undefined" && y.length > 0)
        x +=3D d + y;
    }

    return x;
}



