                $(td).addClass('selected');
                $(o)[0].s =3D $(td);
            }


            $(td).mouseover(function(){
                $(this).addClass('hover');
            });
            $(td).mouseout(function(){
                $(this).removeClass('hover');
            });
            $(td).click(function(){
                if ($(this).hasClass('selected')) {
                    return false;
                } else {
                    createHiddenField ($(o)[0], name, target, obj);
                    $(this).addClass('selected');
                    $(o)[0].s =3D $(this);
                }
            });

            $(td).appendTo(tr);
            k++;
            if (k=3D=3D4 || cnt=3D=3D$(obj).length) {
                k =3D 0;
                $(tr).appendTo(table);
            }
            cnt++;
        });
        var blankpopup =3D $("#popupblank").clone().appendTo(place);
        $(blankpopup).attr('id','ms'+target);
        $(blankpopup).find('.popupcontent').html($(table));
        mstarget =3D $(blankpopup);
