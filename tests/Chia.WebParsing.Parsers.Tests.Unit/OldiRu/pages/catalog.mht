// name - attr NAME for input (hidden fields)
// target - div#id holder for hidden fields
// place - root container for popup (must be css position: relative;)
// el - button (null - initialize: do not show popup; not null - show popup)
    if (!obj || !target) return false;
        var mstarget;
        var pos =3D $("#"+target).position();
    if ($('#ms'+target).length > 0) {
        mstarget =3D $('#ms'+target);
    } else {
        var table =3D $('<table class=3D"multiselect"></table>');

        var k=3D0, cnt=3D1;
        var tr;
        $(obj).each(function() {

            var o =3D $(this);

            if (k=3D=3D0) {
                tr =3D $('<tr></tr>');
            }
            var td =3D $('<td class=3D"msopt">'+$(o)[0].t+'</td>');

            // init default filter
            if ($(o)[0].s =3D=3D 1) {
                createHiddenField ($(o)[0], name, target, obj);
                $(td).addClass('selected');
                $(o)[0].s =3D $(td);
            }


            $(td).mouseover(function(){
                $(this).addClass('hover');
            });
            $(td).mouseout(function(){
                $(this).removeClass('hover');
            });
            $(td).click(function(){
                if ($(this).hasClass('selected')) {
                    return false;
                } else {
                    createHiddenField ($(o)[0], name, target, obj);
                    $(this).addClass('selected');
                    $(o)[0].s =3D $(this);
                }
            });

            $(td).appendTo(tr);
            k++;
            if (k=3D=3D4 || cnt=3D=3D$(obj).length) {
                k =3D 0;
                $(tr).appendTo(table);
            }
            cnt++;
        });
        var blankpopup =3D $("#popupblank").clone().appendTo(place);
        $(blankpopup).attr('id','ms'+target);
        $(blankpopup).find('.popupcontent').html($(table));
        mstarget =3D $(blankpopup);
    }
    if (el) {
        $(mstarget).css({display:'block'});
        var wdt =3D $(mstarget).width();
        var defX =3D $(".all").width() + wdt + 10;
        // close other popup's
        $(".popup",place).each(function(){
            if($(this).attr('id') !=3D $(mstarget).attr('id')) {
                $(this).animate({right: '-'+defX+'px'});
            }
        });

        // and open current popup
        $(mstarget).css({top: pos.top+'px', right: defX + 'px'});
        $(mstarget).animate({ right: '-12px' });
        $(mstarget).find('.closepop').click(function(){
            $(mstarget).animate({right: defX + 'px'});
            return false;
        });
    }
    return false;
}


