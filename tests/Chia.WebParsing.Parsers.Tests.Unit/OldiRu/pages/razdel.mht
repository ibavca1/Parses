function smartPopUp(obj) {
    $(obj).css({display:'block'});
    var wdt =3D $(obj).width();
    var hgt =3D $(obj).height();
    var cenH =3D $(window).width()/2;
    var cenV =3D $(window).scrollTop() + $(window).height()/2;
    $(obj).animate({left: (cenH - wdt/2)+'px',top: (cenV - hgt/2)+'px'});

    $(obj).find(".closepop").click(function(){
        $(obj).animate({left:'-9999px'});
        return false;
    });
    return false;
}



